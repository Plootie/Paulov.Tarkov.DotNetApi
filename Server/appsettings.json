  "ServerMode": "Dev",
  "AllowedHosts": "*",
  "ZOMBIES_ONLY": false,
  "MongoDBConnectionString": "SECRET",
  "ConnectionStrings": {
    "MongoDBConnectionString": "SECRET"
  },
  "GitHubDatabaseUrl": "paulov-t/Paulov.Tarkov.Db",
